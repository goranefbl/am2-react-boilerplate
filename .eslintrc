{
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "plugins": ["prettier"],
    "extends": ["airbnb", "prettier", "prettier/react"],
    "rules": {
        "prettier/prettier": ["error"],
        "react/jsx-indent-props": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "indent": ["error", 4],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
